<Window x:Class="PaintingClass.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PaintingClass"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="600">
    <DockPanel Background="#181A1B">
        <Button x:Name="BackButton" Content="Back" DockPanel.Dock="Bottom" Width="100" Click="Back_Button_Click"/>
        <Button DockPanel.Dock="Bottom" Height="30" Width="60" Click="Skip_Button_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom">
            SKIP
        </Button>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Source="">
        </Frame>
    </DockPanel>
</Window>
