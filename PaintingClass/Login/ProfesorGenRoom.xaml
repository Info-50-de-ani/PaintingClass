<Page
    x:Class="PaintingClass.Login.ProfesorGenRoom"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uc="[uc ]clr-namespace:PaintingClass.UserControls"
    Title="ProfesorGenRoom"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Resources/Login/Images/elevJoin.jpg" />
        </Grid.Background>
        <Button
            Margin="10"
            Padding="2"
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Click="GenRoom_Button_Click"
            Content="Creeaza o camera noua"
            FontSize="25"
            Style="{StaticResource ColorFadeButtonStyle}">
            <Button.Effect>
                <DropShadowEffect BlurRadius="10" ShadowDepth="2" />
            </Button.Effect>
        </Button>
    </Grid>
</Page>
